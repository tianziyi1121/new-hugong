.details-img { width: 100%; height: 362rpx; padding: 0; margin: 0;display:block}
.details-title { width: 100%; height: 220rpx; background: #f5f5f5;}
.details-name { width: 690rpx; height: 268rpx; background: #fff; border-radius: 20rpx; top: -72rpx; left: 30rpx; box-shadow: 0px 10px 24px #999;}
.title-name { margin: 36rpx 0 0 34rpx; font-size: 36rpx; color: #030303; height: 48rpx;}
.title-money { margin: 32rpx 0 0 34rpx; color: #ff0000; font-size: 36rpx;}
.title-line { height: 3rpx; margin: 0 auto; width: 650rpx; background: #cdcdcd; margin-top: 22rpx;}
.title-list { width: 650rpx; margin: 0 auto; }
.title-list image { width: 30rpx; height: 24rpx; vertical-align: middle; margin-right: 6rpx;}
.title-list text { font-size: 24rpx; color: #000;}
checkbox { padding-left: 30rpx;}
checkbox .wx-checkbox-input { width: 36rpx; height: 36rpx;margin-top: -5rpx;}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before { text-align: center; font-size: 30rpx; color:  #37CF6B; background: transparent; transform: translate(-50%, -50%) scale(1); -webkit-transform: translate(-50%, -50%) scale(1);}
/* 图片 */
.details-image { width: 100%; display: block;}
.footer-right { background: #CCC; color: #fff;}
.footerRight { background: #37CF6B; color: #fff;}
/* 图片 */
.image {width:750rpx; height: 710rpx;}
.image1 { width: 750rpx; height: 362rpx;}
.image23 { width: 750rpx; height: 1055rpx;}
/* 列表 */
.details-list-title { font-size: 42rpx; color: #000; margin: 73rpx 0 47rpx;}
.details-list-title text { font-weight: 700;}
.details-subhead { width: 710rpx; margin: 0 auto; margin-bottom: 23rpx;}
.details-subhead view { width: 5rpx; height: 34rpx; background: #37cf6b;font-size: 36rpx; color: #000; margin: 8rpx 10rpx 0 0;}
.details-image { width: 710rpx; margin: 0 auto; margin-bottom: 52rpx;}
.details-image image { width: 341rpx; height: 211rpx;}
.details-image image:nth-child(2) { margin-left: 30rpx;}
/* 按钮 */
.lsdetails-line { height: 88rpx;}
.details-btn { height: 98rpx; line-height: 98rpx; bottom: 0; left: 0; width: 100%; background: #fff;}
.details-btn image { width: 32rpx; height: 32rpx; vertical-align: middle; margin-right: 10rpx;}
.details-btn > view { width: 100%;font-size: 32rpx;}
.details-btn > view:nth-child(1) { color: #37cf6b; border-top: 2rpx solid #ccc;}
/* .details-btn > view:nth-child(2) { background: #37cf6b; color: #fff; } */
/* 弹窗 */
.view-content { width: 100%; height: 340rpx; left: 0; bottom: 0; z-index: 1001;}
.view-hour, .view-affirm, .view-cancel { height: 88rpx; line-height: 88rpx;}
.view-content > view:nth-child(1) { margin: 20rpx 30rpx; border-radius: 16rpx; background: #fff;}
.view-affirm { border-top: 2rpx solid #ccc;}
.view-cancel { margin: 0 30rpx; background: #fff; border-radius: 16rpx;}